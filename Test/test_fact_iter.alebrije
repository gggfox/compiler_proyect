program MyRlike;

function int fact(int: num){
    vars{
        int: accum;
    }
    accum = 1;

    while(num > 1){
        accum = accum * num;
        num = num - 1;
    }

    # fix return
    # return(accum);
    return(4);
}

main(){
    write((2 + fact(6) * fact(2)) * 2);    
}


}

function int fact(){
    vars {
        int : i;
    }
    i = j + (p - j*2+j);
    if(j == 1) {
        return(j);
    }else{
        return(j*fact(j - 1));
    }
}

function void calcula(int: y){
    vars {
        int: x;
    }
    while(x < 11){
        y = y * arreglo[x];
        x = x + 1;
        write(arreglo[x]);
    }
    write("acumulado",y);
}

main(){
